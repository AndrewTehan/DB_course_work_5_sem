<section id="why-us" class="why-us" style="margin-top: 150px">
	<div class="container">
		<div class="row">
			<%@clients.each do |client|%>
				<div class="col-lg-4">
					<div class="box" style="text-align: center">
						<span><%=client["client_first_name"]%> <%=client["client_last_name"]%></span>
							<p><%=client["client_email"]%></p>
						<span><%=client["client_phone_number"]%></span>
					</div>
				</div>
			<%end%>
		</div>
	</div>
</section>
